            {
              "operating_system": [
                {
                  "description": "FortiWeb: (<expression> <version>)",
                  "cpe": "cpe:2.3:o:fortinet:fortiweb:*:*:*:*:*:*:*:*",
                  "affected": {
                    "le": "6.0.0"
                  },
                  "fixed_in": "6.0.1"
                }
              ]
            }
          ],
          "impact": "WAF bypass (URL based attack)"
        }
      ]
    }
